#!! ## Giellatekno late rules

LIST IDNOUN = "artikkel" "kapittel" "paragraf" "punkt" ;

SELECT:SemID Sem/ID IF (-1 IDNOUN OR (":"))(0 Num);

#!! ### Neuter sg pl

REMOVE Neu + Pl IF (-1 BOS OR PUNCT)(0 Neu + Sg)(1 EOS OR PUNCT);

REMOVE Neu + Pl IF (0 Neu + Sg)(1 CC)(2 N + Sg);
REMOVE Neu + Sg IF (-1 ("flere"))(0 Neu + Pl);
REMOVE Neu + Sg IF (*-1 ("flere") OR ("mange") BARRIER V)(0 Neu + Pl);


#!! ### Pronouns

REMOVE (Acc) IF (-1 CS)(1 Prs OR Prt);

REMOVE (Pron) IF (0 Det + $$GEN)(*1 N + $$GEN BARRIER NOT-PRE-NP-HEAD);	

#!! ###  Det rules

REMOVE Pron + Pers + Pl3 IF (1 N + Pl OR A + Pl OR Qnt + Pl);

REMOVE Det IF (0 Pron)(NEGATE *1 N OR Num BARRIER NOT-NPMOD);
REMOVE Det IF (0 Pron)(0 Det + Neu)(*1C N + Msc OR N + Fem BARRIER NOT-NPMOD);
REMOVE Det IF (0 Pron)(1 Pr OR V OR CLB OR EOS);

# N and not V
REMOVE (Pl) IF (-1 Pr)(0 Sg);
      ## i vater

REMOVE (Pl) IF (-1 Det + Sg)(0 Sg);
      ## i vater

REMOVE A + Pos + Def IF (-1 N OR Pron)(0 Prt);

#!! ###  V and not N

REMOVE N + Neu + Sg + Def IF (-2 PrfPrc)(-1 CC);
REMOVE N + Pl + Indef IF (0 Prs)(1 IM)(2 Inf);
## han prøver å sykle

#!! ###  Prepositions

# Pr or Adv OR CS
REMOVE Adv IF (0 Pr)(1 Det OR A OR Qnt OR N);
REMOVE CS IF (0 Pr)(*1 EOS OR CLB BARRIER Ind);


#!! ###  Late rules, Gt

# Han arbeider som bud.
REMOVE:SomPr Pl IF (-1 %som%-Pr)(0 N + Sg);
REMOVE:SomCS Pr IF (0 %som%-CS)(*1 V);

REMOVE:LexAdv A + Der/AAdv IF (0 Adv);



#!! ###  Rules with weights
#!! **minweight** selects reading with lowest weight.
SELECT:minweight (<W=MIN>);



