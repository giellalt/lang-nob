LIST QMARK = """ ; 			  #!!≈ - @CODE@
SET PUNCT = PPUNCT - QMARK ;  #!!≈ - @CODE@
# ADD " FOR SYNTAX COLOURING.

#!! ### Sets for Semantic tags
# -------------

LIST Sem/Ani = Sem/Ani ;	   #!!≈ - @CODE@
LIST Sem/Date = Sem/Date ;	   #!!≈ - @CODE@
LIST Sem/Fem = Sem/Fem ;	   #!!≈ - @CODE@
LIST Sem/Group = Sem/Group ;   #!!≈ - @CODE@
