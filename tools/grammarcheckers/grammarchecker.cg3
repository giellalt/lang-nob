LIST Pos = Pos ;		     #!!≈ - @CODE@
LIST Comp = Comp ;		     #!!≈ - @CODE@
LIST Superl = Superl ;	     #!!≈ - @CODE@
LIST Sg = Sg ;			     #!!≈ - @CODE@
LIST Pl = Pl ;			     #!!≈ - @CODE@
LIST Def = Def ;		     #!!≈ - @CODE@
SET CC-CS = CC OR CS ;	     #!!≈ - @CODE@
LIST ABBR = ABBR ;		     #!!≈ - @CODE@
LIST ACR = ACR ;		     #!!≈ - @CODE@
LIST Msc = Msc ;		     #!!≈ - @CODE@
LIST Fem = Fem ;		     #!!≈ - @CODE@
LIST Neu = Neu ;		     #!!≈ - @CODE@
LIST MF = MF ;			     #!!≈ - @CODE@
LIST Pers = Pers ;		     #!!≈ - @CODE@
LIST Dem = Dem ;		     #!!≈ - @CODE@
LIST Interr = Interr ;	     #!!≈ - @CODE@
LIST Indef = Indef ;	     #!!≈ - @CODE@
LIST Pass = Pass ;		     #!!≈ - @CODE@
LIST Recipr = Recipr ;	     #!!≈ - @CODE@
LIST Refl = Refl ;		     #!!≈ - @CODE@
LIST Rel = Rel ;		     #!!≈ - @CODE@
